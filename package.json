{
  "name": "qpid-ember-base",
  "version": "2.1.0",
  "description": "Base styling and layout for Qpid's Ember apps",
  "keywords": [
    "ember-addon"
  ],
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EvicoreQpidHealth/qpid-ember-base"
  },
  "license": "MIT",
  "scripts": {
    "build": "node_modules/ember-cli/bin/ember build",
    "start": "node_modules/ember-cli/bin/ember server",
    "test": "node_modules/ember-cli/bin/ember test --test-port=0",
    "build:development": "node_modules/ember-cli/bin/ember build --environment=development",
    "build:test": "node_modules/ember-cli/bin/ember build --environment=test",
    "build:production": "node_modules/ember-cli/bin/ember build --environment=production"
  },
  "dependencies": {
    "bower": "1.8.0",
    "ember-cli-babel": "^6.9.2",
    "ember-cli-google-fonts": "2.3.1",
